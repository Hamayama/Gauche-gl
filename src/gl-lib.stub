       case 4: glColor4dv(val); break;
     }
  }
  SCM_RETURN(SCM_UNDEFINED);
 badarg:
  Scm_Error(\"bad argument for color: %S, must be an uniform vector of length 3 or 4\", v);
  SCM_RETURN(SCM_UNDEFINED);")

