  } else if (SCM_F64VECTORP(m) && SCM_F64VECTOR_SIZE(m) == 16) {
     glLoadMatrixd(SCM_F64VECTOR_ELEMENTS(m));
  } else {
     Scm_Error(\"f32vector or f64vector of length 16 is required, but got %S\",
               m);
  }
  SCM_RETURN(SCM_UNDEFINED);")

